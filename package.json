    "test-routes": "mocha -r ts-node/register tests/routestests/*.ts",
    "lint": "eslint . --ext .ts",
    "coverage": "nyc npm run test",
    "create-db": "node ./create_db.js",
    "build:dev": "tsc",
    "build:prod": "npm test && npm run lint && tsc"
