    "url": "https://github.com/skatos-ux/Loaner/issues"
  },
  "homepage": "https://github.com/skatos-ux/Loaner#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.14",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "chai": "^4.2.0",
    "eslint": "^7.15.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "sqlite3": "^5.0.0"
