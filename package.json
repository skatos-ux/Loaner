{
  "name": "loaner",
  "version": "1.0.0",
  "description": "Loaner allows you to track who borrowed your devices",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "start:dev": "nodemon",
    "start:prod": "node ./dist/index.js",
